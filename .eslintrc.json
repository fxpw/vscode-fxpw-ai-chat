{
	"env": {
		"browser": false,
		"commonjs": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"globals": {
		"document": "readonly",
		"window": "readonly",
		"hljs": "readonly",
		"navigator": "readonly",
		"marked": "readonly",
		"toHomeButtonOnClickResponse": "readonly",
		"conversationSendTextButtonOnClickResponse": "readonly",
		"deleteChatButtonOnClickResponse": "readonly",
		"addChatButtonOnClickResponse": "readonly",
		"createConversationBody": "readonly",
		"createOpenConversationButton": "readonly",
		"updateBody": "readonly",
		"updateConversationsList": "readonly",
		"getCurrentChatIDRequest": "readonly",
		"clickToOpenConversationButton": "readonly",
		"loadViewOnLoadRequest": "readonly",
		"IN_CHAT": "readonly",
		"vscode": "readonly",
		"CURRENT_CHAT_ID": "readonly",
		"IS_CHAT_BLOCKED": "readonly",
		"goToHome": "readonly",
		"acquireVsCodeApi": "readonly"
	},
	"rules": {
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"no-unreachable": "warn",
		"no-unused-vars": "warn",
		"constructor-super": "warn",
		"valid-typeof": "warn"
	}
}
