{
	"env": {
		"browser": false,
		"commonjs": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"globals": {
		"document": "readonly",
		"window": "readonly",
		"hljs": "readonly",
		"navigator": "readonly",
		"marked": "readonly",
		"acquireVsCodeApi": "readonly"
	},
	"rules": {
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"no-unreachable": "warn",
		"no-unused-vars": "warn",
		"constructor-super": "warn",
		"valid-typeof": "warn"
	}
}
